enum Permission {
  ADMIN
  USER
  PERMISSIONUPDATE
  ADDSONG
  DELETESONG
  UPDATESONG
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission]
}

type Song {
  id: ID! @unique
  title: String!
  description: String!
  image: String!
  song: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User!
}
